body {background: url(../Images/grass.png) repeat-x bottom, linear-gradient(#E0FFFF, green);}

#div1 {position: relative;
		color: red;
		font-size: 1.5em;
		font-weight: bold;
		padding-top: 20px;
		padding-bottom: 20px;
		animation: move1 5s infinite;
		animation-timing-function: linear;}
#div4 {position: center;}		
#div2 {color: white;
		font-size: 1.5em;
		font-weight: bold;
		font-size: 1.5em;
		padding-top: 20px;
		padding-bottom: 50px;
		text-align: center;}
#div3 {width: 200px;
		height: 200px;
		margin: auto;
		background-image: url(../Images/bunny.png);
		background-size: 100%;
		transition: transform 2s, width 2s, height 2s;}	
#div3:hover {transform: rotateY(720deg);}
		
#div5 {width: 100px;
		height: 100px;
		background-image: url(../Images/chick.gif);
		background-size: 100%;
		position: fixed;
		animation: move1 5s infinite;
		animation-timing-function: linear;}
@keyframes move1 {
	0% {top: 0%; left: 45%;}
	25% {top: 45%; left: 0%;}
	50% {top: 90%; left: 45%;}
	75% {top: 45%; left: 90%;}
	100% {top: 0%; left: 45%;}
}

