#office {width: 800px;
		height: 600px;
		background-color: #57ba98;
		color: #f2f2f2;
		padding: 100px;
		border: 30px dashed #182628;
		margin: 100px;
		overflow: scroll;}
#desk{width: 300px;
		height: 300px;
		background-color: #f2f2f2;
		color:3b945e;
		padding: 10px;
		border: 10px outset #deb887;
		margin: 0;
		float: right;
		background-image: url(../Images/grizzly.jpg);
		background-repeat: no-repeat;
		background-position: top right;
		background-origin: content-box;}
p{background-color: #deb887;
	color:3b945e;
	width: 150px;
	height: 150px;
	border: 5px outset #f2f2f2;
	position: relative;
	left: 70px;
	top: 50px;}
h2{background-color: #f2f2f2;
	color:3b945e;
	text-align: center;
	clear: right;}
ul{padding: 0;}
li{display: inline-block;
	background-color: #f2f2f2;
	color:3b945e;
	width: 200px;
	height: 100px;
	padding: 10px;
	margin: 5px;}
	
*{box-sizing: border-box;}
		